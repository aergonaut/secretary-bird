{
  "name": "secretary-bird",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "Chris Fung <chris.fung@coupa.com>",
  "license": "ISC",
  "repository": "https://github.com/coupa/secretary-bird.git",
  "homepage": "https://github.com/coupa/secretary-bird",
  "bugs": "https://github.com/coupa/secretary-bird/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
